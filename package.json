{
  "name": "member-platform",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "watchify -o _amd/js/bundle.js -v -d _amd/js/app.coffee",
    "build": "browserify . -t [envify --NODE_ENV production] | uglifyjs -cm > _amd/js/bundle.min.js"
  },
  "author": "Richard Samuelsson",
  "browserify": {
    "transform": [
      "coffeeify",
      "envify"
    ]
  },
  "dependencies": {
    "hbs": "~2.9.0",
    "underscore": "~1.8.2",
    "backbone": "~1.1.2",
    "jquery": "~2.1.3",
    "bootstrap": "~3.3.4"
  },
  "devDependencies": {
    "browserify": "^6.2.0",
    "envify": "^3.0.0",
    "uglify-js": "~2.4.15",
    "coffeeify": "~1.0.0",
    "watchify": "^2.1.1"
  }

}
